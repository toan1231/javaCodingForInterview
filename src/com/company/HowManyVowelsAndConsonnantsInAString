package makingMoneyOnline;

import java.util.Arrays;

import makingMoneyOnline.Main.toan;

public class HowManyVowelsAndConsonnantsInAString {

	public static void main(String[] args) {
		
String name="aaebcdhiklmn";//a i e o u
int count=0;
for(int i=0;i<name.length();i++) {
	if(name.charAt(i)=='a'||name.charAt(i)=='i'||name.charAt(i)=='e'||name.charAt(i)=='o'||name.charAt(i)=='u') {
		count++;
		
	}
	
}
System.out.println(" count of vowel: "+count);
System.out.println("count of cosonent: "+(name.length()-count));
}
}
	


