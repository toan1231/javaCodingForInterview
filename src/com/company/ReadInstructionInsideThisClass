package makingMoneyOnline;

import java.util.Arrays;
import java.util.HashMap;
import java.util.Map;

import makingMoneyOnline.Main.toan;

public class ReadInstructionInsideThisClass {

	public static void main(String[] args) {
		//instruction: given a number called target, find element in array that add up to that target
int b[]= {1,3,4,5,2,2};

int target=5;
int[]result=getNumberesult(b,target);
System.out.println(result[0]+" "+result[1]);

}
	public static int[] getNumberesult(int[]a,int target){
		int num=1;
		Map<Integer,Integer>map=new HashMap<>();
		int []b= {};
		for(int i=0;i<a.length;i++) {
			for(int j=0;j<a.length;j++) {
				if(a[i]+a[j]==target) {
				
					return new int[] {i,j};
				}
				
			}
		}
		return new int[] {-1,-1};
		
		
		
		
	}
}

